{
  "type": "module",
  "name": "slate-to-react",
  "description": "React component and utilities to transform Slate nodes to React",
  "version": "0.3.0",
  "author": "Nick K.",
  "license": "MIT",
  "keywords": [
    "slate",
    "react",
    "render",
    "view",
    "react-component",
    "component",
    "hook",
    "react-hook"
  ],
  "repository": "octet-stream/slate-to-react",
  "sideEffects": false,
  "source": "src/slate-to-react.ts",
  "main": "lib/slate-to-react.cjs",
  "umd:main": "lib/slate-to-react.umd.js",
  "module": "lib/slate-to-react.legacy.mjs",
  "types": "@type/slate-to-react.d.ts",
  "exports": {
    "import": "./lib/slate-to-react.mjs",
    "require": "./lib/slate-to-react.cjs",
    "types": "./@type/slate-to-react.d.ts",
    "default": "./lib/slate-to-react.mjs"
  },
  "scripts": {
    "eslint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:types": "tsc --noEmit",
    "test": "ava",
    "coverage": "c8 npm test",
    "report": "c8 -r=html npm test",
    "ci": "c8 npm test && c8 report --reporter=json",
    "build": "microbundle --tsconfig tsconfig.build.json --jsx React.createElement --jsxFragment React.Fragment",
    "cleanup": "del-cli @type lib",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && npm run eslint && npm run lint:types && npm run cleanup && npm run build",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "6.2.2",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "ava": "5.2.0",
    "c8": "7.13.0",
    "del-cli": "5.0.0",
    "eslint": "8.35.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-ava": "14.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "global-jsdom": "8.7.0",
    "husky": "8.0.3",
    "jsdom": "21.1.0",
    "lint-staged": "13.1.2",
    "microbundle": "0.15.1",
    "pinst": "3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "slate": "0.91.4",
    "slate-react": "0.91.8",
    "ts-expect": "1.3.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "slate": "^0.91.x",
    "slate-react": "^0.91.x"
  },
  "dependencies": {
    "nanoid": "3.3.4"
  }
}
